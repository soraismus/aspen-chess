{ div } = require('react').DOM

MainView = \props ->
  { color, height, msg, width } = props

  color  = color  || 'red'
  height = height || '100px'
  msg    = msg    || 'Hello'
  width  = width  || '100px'

  div
    style:
      backgroundColor: color
      height: height
      width: width
    msg

module.exports = MainView
