{ div }    = require('react').DOM
# _h1
Pagination = require './pagination'
UserList   = require './user_list'

UserDisplay = \props ->
  { pagination, user_list } = props

  div (
    { id: 'main-container' } # Bootstrap aspect.
    _h1 'All Users'
    Pagination pagination
    UserList user_list
    Pagination pagination
  )

module.exports = UserDisplay
